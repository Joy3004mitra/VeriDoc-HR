    </script>
    <script src="~/Scripts/windowscroll.js"></script>

    <script>
        $(window).scroll(function () {
            $('video').each(function () {
                if ($(this).visible(true)) {
                    $(this)[0].play();
                } else {
                    $(this)[0].pause();
                }
            })
        });
